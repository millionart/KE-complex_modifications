{
  "title": "Keymaps in Adobe Photoshop | 在 Photoshop 中更改键盘布局",
  "rules": [
    {
      "description": "Swap control option and command | 切换 ⌃(control) ⌥(option) ⌘(command) 的位置 (Win 布局)",
      "manipulators": [
        {"type": "basic", "from": {"key_code": "left_command", "modifiers": {"optional": ["any"]}},"to": [{"key_code": "left_option"}],"conditions": [{"type": "frontmost_application_if","bundle_identifiers": ["^com\\.adobe\\.Photoshop$"]}]},
        {"type": "basic", "from": {"key_code": "left_control", "modifiers": {"optional": ["any"]}},"to": [{"key_code": "left_command"}],"conditions": [{"type": "frontmost_application_if","bundle_identifiers": ["^com\\.adobe\\.Photoshop$"]}]},
        {"type": "basic", "from": {"key_code": "left_option", "modifiers": {"optional": ["any"]}},"to": [{"key_code": "left_control"}],"conditions": [{"type": "frontmost_application_if","bundle_identifiers": ["^com\\.adobe\\.Photoshop$"]}]}
      ]
    },
    {
      "description": "OS style Undo / Redo | 系统风格撤销/重做快捷键",
      "manipulators": [
        {"type": "basic", "from": {"key_code": "z", "modifiers": {"mandatory": ["left_option"]}},"to": [{"key_code": "z","modifiers": ["left_option","left_command"]}],"conditions": [{"type": "frontmost_application_if","bundle_identifiers": ["^com\\.adobe\\.Photoshop$"]}]},
        {"type": "basic", "from": {"key_code": "y", "modifiers": {"mandatory": ["left_option"]}},"to": [{"key_code": "z","modifiers": ["left_command","left_shift"]}],"conditions": [{"type": "frontmost_application_if","bundle_identifiers": ["^com\\.adobe\\.Photoshop$"]}]},
        {"type": "basic", "from": {"key_code": "z", "modifiers": {"mandatory": ["left_shift","left_command"]}},"to": [{"key_code": "z","modifiers": ["left_command"]}],"conditions": [{"type": "frontmost_application_if","bundle_identifiers": ["^com\\.adobe\\.Photoshop$"]}]},
        {"type": "basic", "from": {"key_code": "z", "modifiers": {"mandatory": ["left_option","left_control"]}},"to": [{"key_code": "y","modifiers": ["left_command"]}],"conditions": [{"type": "frontmost_application_if","bundle_identifiers": ["^com\\.adobe\\.Photoshop$"]}
          ]
        }
      ]
    },
